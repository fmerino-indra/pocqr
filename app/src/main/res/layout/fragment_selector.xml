<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frSelector"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.SelectorFragment">

    <Button
        android:id="@+id/btnResponsible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btnBrother"
        android:text="Responsible"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="158dp" />
    <Button
        android:id="@+id/btnBrother"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnResponsible"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="Brother"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="158dp" />
    <Button
        android:id="@+id/btnManage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnBrother"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="Manage"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="158dp" />
</androidx.constraintlayout.widget.ConstraintLayout>